<p id="notice"><%= notice %></p>

<h1>Listing Postings</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Lattitude</th>
      <th>Longitude</th>
      <th>Title</th>
      <th>Description</th>
      <th>Date</th>
      <th>Time</th>
      <th>Length of job</th>
      <th>Rate of pay</th>
      <th>Expires</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @postings.each do |posting| %>
      <tr>
        <td><%= posting.location %></td>
        <td><%= posting.lattitude %></td>
        <td><%= posting.longitude %></td>
        <td><%= posting.title %></td>
        <td><%= posting.description %></td>
        <td><%= posting.date %></td>
        <td><%= posting.time %></td>
        <td><%= posting.length_of_job %></td>
        <td><%= posting.rate_of_pay %></td>
        <td><%= posting.expires %></td>
        <td><%= link_to 'Show', posting %></td>
        <td><%= link_to 'Edit', edit_posting_path(posting) %></td>
        <td><%= link_to 'Destroy', posting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Posting', new_posting_path %>
